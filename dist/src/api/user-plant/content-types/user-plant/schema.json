        },
        "waterTimes": {
            "type": "integer",
            "min": 1
        },
        "waterRange": {
            "type": "enumeration",
            "enum": [
                "hours",
                "days",
                "weeks",
                "months"
            ]
        },
        "sunTimes": {
            "type": "integer",
            "min": 1
        },
        "sunRange": {
            "type": "enumeration",
            "enum": [
                "hours",
                "days",
                "weeks",
                "months"
            ]
        }
    }
